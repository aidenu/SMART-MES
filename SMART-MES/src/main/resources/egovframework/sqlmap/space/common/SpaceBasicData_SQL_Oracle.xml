<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap PUBLIC "-//iBATIS.com//DTD SQL Map 2.0//EN" "http://www.ibatis.com/dtd/sql-map-2.dtd">

<sqlMap namespace="SpaceBasicData">

	
	<resultMap id="roleDataList" class="java.util.HashMap">
		<result property="AUTHOR_CODE" column="AUTHOR_CODE" />
	</resultMap>

 	 <parameterMap id="paramMapRoleData" class="java.util.HashMap">
		<parameter property="userid" jdbcType="VARCHAR" javaType="java.lang.String" mode="IN" />
		<parameter property="out_data" jdbcType="ORACLECURSOR" javaType="java.sql.ResultSet" mode="OUT" />
	</parameterMap>
	
	<procedure id="selectUserRoleData" parameterMap="paramMapRoleData" resultMap="roleDataList">
		<![CDATA[
        	{call SPC_Q_BASIC_DATA.getUserRoleData_Pro(?, ?)}
        ]]>
	</procedure>
	
	

	<resultMap id="basicDataList" class="java.util.HashMap">
		<result property="child_id" column="CHILD_ID" />
		<result property="parent_id" column="PARENT_ID" />
		<result property="child_value" column="CHILD_VALUE" />
		<result property="level_num" column="LEVEL_NUM" />
	</resultMap>

 	 <parameterMap id="paramMapBasicData" class="java.util.HashMap">
		<parameter property="actionlevel" jdbcType="VARCHAR" javaType="java.lang.String" mode="IN" />
		<parameter property="parentid" jdbcType="VARCHAR" javaType="java.lang.String" mode="IN" />
		<parameter property="out_data" jdbcType="ORACLECURSOR" javaType="java.sql.ResultSet" mode="OUT" />
	</parameterMap>
	
	<procedure id="selectBasicDataList" parameterMap="paramMapBasicData" resultMap="basicDataList">
		<![CDATA[
        	{call SPC_Q_BASIC_DATA.getBasicDataList_Pro(?, ?, ?)}
        ]]>
	</procedure>
	
	
	
	<resultMap id="basicDataResult" class="java.util.HashMap">
		<result property="out_data" column="ACTION_RESULT" />
	</resultMap>
	
	 <parameterMap id="paramMapBasicDataAction" class="java.util.HashMap">
		<parameter property="actiontype" jdbcType="VARCHAR" javaType="java.lang.String" mode="IN" />
		<parameter property="userid" jdbcType="VARCHAR" javaType="java.lang.String" mode="IN" />
		<parameter property="actionlevel" jdbcType="VARCHAR" javaType="java.lang.String" mode="IN" />
		<parameter property="parentid" jdbcType="VARCHAR" javaType="java.lang.String" mode="IN" />
		<parameter property="childid" jdbcType="VARCHAR" javaType="java.lang.String" mode="IN" />
		<parameter property="childvalue" jdbcType="VARCHAR" javaType="java.lang.String" mode="IN" />
 		<parameter property="out_data" jdbcType="ORACLECURSOR" javaType="java.sql.ResultSet" mode="OUT" />
	</parameterMap>
	
	<procedure id="updateBasicDataList" parameterMap="paramMapBasicDataAction" resultMap="basicDataResult">
		<![CDATA[
        	{call SPC_Q_BASIC_DATA.setBasicData_Pro(?, ?, ?, ?, ?, ?, ?)}
        ]]>
	</procedure>
		
</sqlMap>
